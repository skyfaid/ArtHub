<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.Activities.UserActivity"
            layoutX="5.0" layoutY="5.0" prefHeight="570.0" prefWidth="1091" stylesheets="@/css/csskolchay.css">
    <children>
        <ScrollPane fitToWidth="true" fitToHeight="true" AnchorPane.topAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0">
            <content>
                <TilePane fx:id="tilePaneActivities" prefColumns="2" hgap="10" vgap="10" styleClass="tile-pane" stylesheets="@/css/csskolchay.css">
                    <padding>
                        <Insets top="10" right="10" bottom="10" left="10"/>

                    </padding>
                </TilePane>
            </content>
        </ScrollPane>

    </children>
    <VBox fx:id="topParticipantsContainer">
        <Label text="Top Participants" />
        <!-- Dynamic content will be added here -->
    </VBox>
</AnchorPane>
