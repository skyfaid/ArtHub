<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.Events.EditParticipantController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TextField fx:id="txtNom" layoutX="150" layoutY="50" styleClass="text-field" />
        <TextField fx:id="txtPrenom" layoutX="150" layoutY="100" styleClass="text-field" />
        <Button text="Save" onAction="#saveParticipant" layoutX="150" layoutY="150" styleClass="button" />
        <VBox fx:id="notificationContainer" layoutX="100" layoutY="100" />
    </children>
    <stylesheets>
        <URL value="@/assets/stylesmootaz.css" />
    </stylesheets>
</AnchorPane>
